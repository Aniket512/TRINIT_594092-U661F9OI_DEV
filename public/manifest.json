{
  "version": "1.0.0",
  "name": "Carbonfootprint calculator",
  "manifest_version": 2,

  "icons": {
		"192": "logo192.png"
	},
  "browser_action": {
		"default_icon": "logo192.png",
		"default_title": "Click here to see your Carbon Footprint of Website",
		"default_popup": "index.html"
	},
	"permissions": ["tabs", "storage", "webRequestBlocking","webNavigation", "webRequest", "<all_urls>"]
}